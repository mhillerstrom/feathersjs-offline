{
  "name": "@feathersjs-offline/common",
  "description": "Common utilities for offline data access to FeathersJS app.",
  "version": "0.0.1",
  "homepage": "https://feathersjs.com",
  "main": "lib/",
  "types": "lib/",
  "keywords": [
    "feathers",
    "feathers-plugin"
  ],
  "license": "MIT",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/feathers"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mhillerstrom/feathersjs-offline/owndata.git"
  },
  "author": {
    "name": "Michael Hillerstroem",
    "email": "michael@hillerstrom.name",
    "url": "https://github.com/mhillerstrom"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/mhillerstrom/feathersjs-offline/issues"
  },
