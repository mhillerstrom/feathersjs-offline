  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.2",
    "mocha": "^8.1.3",
    "shx": "^0.3.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
