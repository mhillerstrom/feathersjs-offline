{
  "name": "feathersjs-offline",
  "description": "FeathersJS offline realtime adapter wrappers",
  "version": "0.0.1",
  "homepage": "https://github.com/mhillerstrom/feathersjs-offline",
  "main": "dist/",
  "keywords": [
    "feathers",
    "feathersjs-offline"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/mhillerstrom/feathersjs-offline.git"
  },
  "author": {
    "name": "Feathersjs-offline contributors",
    "email": "hello(at)hillerstrom.name",
    "url": "https://feathersjs.com"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/mhillerstrom/feathersjs-offline/issues"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "install": "lerna bootstrap",
    "publish": "lerna publish && git commit -am \"chore: Update changelog\" && git push origin",
    "lint": "tslint 'packages/**/*.js' 'packages/**/*.ts' -c tslint.json --fix",
    "update-dependencies": "ncu -u && lerna exec -- ncu -u",
    "clean": "find . -name node_modules -exec rm -rf '{}' + && find . -name package-lock.json -exec rm -rf '{}' + && find . -name .scratch -exec rm -rf '{}' +",
    "test": "npm run lint && nyc lerna run test",
    "webpack": "parallel-webpack"
  },
  "semistandard": {
    "env": [
      "mocha"
    ]
  },
  "directories": {
    "lib": "lib"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "lerna": "^3.22.1",
    "npm-check-updates": "^9.2.3",
    "nyc": "^15.1.0",
    "parallel-webpack": "^2.6.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5",
    "webpack": "^4.44.2",
    "webpack-merge": "^5.3.0"
  }
}
